@model Entities.Concrete.Admin
@{
    ViewBag.Title = "UpdateAdmin";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h2>Admin Güncelle</h2>
<br />
@using (Html.BeginForm("UpdateAdmin", "AdminList", FormMethod.Post))
{
  
    @Html.HiddenFor(x => x.adminId, new { @class = "form-control" })
    <br />
    @Html.Label("İsim")
    @Html.TextBoxFor(x => x.adminName, new { @class = "form-control" })
    <br />
    @Html.Label("Soyisim")
    @Html.TextBoxFor(x => x.adminSurname, new { @class = "form-control" })
    <br />
    @Html.Label("Mail")
    @Html.TextBoxFor(x => x.adminMail, new { @class = "form-control" })
    <br />
    @Html.Label("Şifre")
    @Html.TextBoxFor(x => x.adminPassword, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Admin Güncelle</button>
}